{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
    ],
    "module": "commonjs",
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "types": ["jest", "node"]
  },
  "include": ["src"],
  "exclude": ["node_modules", "build"]
}
